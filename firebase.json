        "dart": {
            "lib/firebase_options.dart": {
            "projectId": "nas-app-service",
            "configurations": {
                "android": "1:174371785389:android:00ef7721c53c22ac781424",
                "ios": "1:174371785389:ios:ab4af9095a2e9110781424",
